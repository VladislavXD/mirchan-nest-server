{
"$schema": "https://json.schemastore.org/nest-cli",
"collection": "@nestjs/schematics",
"sourceRoot": "src",
"compilerOptions": {
  "deleteOutDir": true,
  "assets": [
    {
      "include": "../__generated__/**/*",
      "outDir": "dist/__generated__",
      "watchAssets": true
    }
  ],
  "watchAssets": true
}
}
